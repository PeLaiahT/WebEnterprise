@{
	ViewData["Title"] = "Home Page";
	Layout = "~/Views/Shared/_layoutHome.cshtml";
}

<nav class="navbar navbar-expand-lg navbar-dark bg-dark d-flex justify-content-between">
	<a class="navbar-brand" asp-action="Index" asp-controller="Home">KATT</a>
	<form class="form-inline" asp-area="Identity" asp-page="/Account/Login">
		<button class="btn btn-outline-success my-2 my-sm-0" type="submit">Login</button>
	</form>
</nav>
<div class="text-center">
	<h1 class="text-info">Welcome</h1>
</div>

<div id="carouselExampleIndicators" class="carousel slide w-75 mx-auto" data-ride="carousel">
	<ol class="carousel-indicators">
		<li data-target="#carouselExampleIndicators" data-slide-to="0" class="active"></li>
		<li data-target="#carouselExampleIndicators" data-slide-to="1"></li>
	</ol>
	<div class="carousel-inner">
		<div class="carousel-item active">
			<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcT3fFWgFXAlBJoCyFrrBxabmwORcvVBM08Mag&usqp=CAU" class="d-block w-100" alt="...">
		</div>
		<div class="carousel-item">
			<img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTiIDtpInesOe3Hzfk-53m2u1yYyqfnqEBT2g&usqp=CAU" class="d-block w-100" alt="...">
		</div>
	</div>
	<button class="carousel-control-prev" type="button" data-target="#carouselExampleIndicators" data-slide="prev">
		<span class="carousel-control-prev-icon" aria-hidden="true"></span>
		<span class="sr-only">Previous</span>
	</button>
	<button class="carousel-control-next" type="button" data-target="#carouselExampleIndicators" data-slide="next">
		<span class="carousel-control-next-icon" aria-hidden="true"></span>
		<span class="sr-only">Next</span>
	</button>
</div>
<script>
	$('.carousel').carousel()
</script>
@model IEnumerable<WebEnterprise.Models.DTO.DocsIdea>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="write-post w-100">
            <div class="f-card d-flex justify-content-between">
              <div class="d-flex">
                    <img class="img-profile rounded-circle pr-2" style="height:40px;" src="data:image/jpeg;base64," />
 
                    <textarea class="form-control" id="validationTextarea" style="height: 40px;width: 500px;" placeholder="What do you thing about ???"></textarea>
                </div>
                <div>            
                    <button type="button" class="btn btn-info"><a asp-action="Create" class="text-decoration-none text-white">Post</a></button>
                </div>
            </div>
        </div>
